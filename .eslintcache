p/Col\"\nimport Container from \"react-bootstrap/Container\"\nimport Row from \"react-bootstrap/Row\"\nimport Dropdown from \"react-bootstrap/Dropdown\"\n\nclass DisplayList extends Component {\n  state = {\n    checkedOut: [],\n  }\n\n  // remove song from checkedOut array within the state\n  removeSong = (song) => {\n    const checkedOut = this.state.checkedOut.filter((s) => s.name !== song.name)\n    this.setState({\n      checkedOut,\n    })\n  }\n\n  // place song in checkedOut array within the state\n  addSong = (song) => {\n    if (\n      this.state.checkedOut.filter((s) => s.name === song.name).length === 0\n    ) {\n      let checkedOut = this.state.checkedOut.concat(song)\n      this.setState({\n        checkedOut,\n      })\n    }\n  }\n\n  // generate html cards for all songs\n  htmlAllSongs = (song) => {\n    return (\n      <Card key={song.id}>\n        <Card.Img\n          variant=\"top\"\n          src={song.image}\n          // src={require(`../assets/${song.image}.jpg`)}\n        />\n        <Card.Body>\n          <Card.Title>{song.name}</Card.Title>\n          <Card.Text>Genre: {song.genre}</Card.Text>\n          <Card.Text>Price: {song.price}</Card.Text>\n          <Card.Text>Days since release: {song.days}</Card.Text>\n        </Card.Body>\n        <Button variant=\"success\" onClick={() => this.addSong(song)}>\n          Add to My Songs\n        </Button>\n        <Dropdown.Divider />\n      </Card>\n    )\n  }\n\n  // generate html cards for all songs added by user\n  htmlCheckedOutSongs = (song) => {\n    return (\n      <Card key={song.id}>\n        <Card.Img variant=\"top\" src={song.image} />\n        <Card.Body>\n          <Card.Title>{song.name}</Card.Title>\n          <Card.Text>Genre: {song.genre}</Card.Text>\n          <Card.Text>Price: {song.price}</Card.Text>\n          <Card.Text>Days since release: {song.days}</Card.Text>\n        </Card.Body>\n        <Button variant=\"danger\" onClick={() => this.removeSong(song)}>\n          Remove from My Songs\n        </Button>\n      </Card>\n    )\n  }\n\n  render() {\n    const all = this.props.songsToShow\n    const allHtml = all.map(this.htmlAllSongs)\n    const checkedOut = this.state.checkedOut\n    const checkedOutHtml = checkedOut.map(this.htmlCheckedOutSongs)\n    let priceTotal = 0\n    const findPriceTotal = this.state.checkedOut.forEach((n) => {\n      priceTotal += n.price\n    })\n\n    return (\n      <Container>\n        <Row>\n          <Col>\n            <Card>\n              <Card.Header>\n                Number of songs that match your selections:\n              </Card.Header>\n              <Card.Body>{all.length}</Card.Body>\n            </Card>\n            {allHtml}\n          </Col>\n          <Col>\n            <h1>My Songs</h1>\n            <h2>${priceTotal}</h2>\n            {checkedOutHtml}\n          </Col>\n        </Row>\n      </Container>\n    )\n  }\n}\n\nexport default DisplayList\n","/Users/benjaminK/WorkBubble/sem3/cs1300/net_ninja/react/dev-bootstrap/src/components/images.js",[],{"ruleId":"24","severity":1,"message":"25","line":81,"column":11,"nodeType":"26","messageId":"27","endLine":81,"endColumn":25},"no-unused-vars","'findPriceTotal' is assigned a value but never used.","Identifier","unusedVar"]